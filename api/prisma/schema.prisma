generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Profile {
    id              String   @id @db.Uuid
    email           String
    hashed_password String
    first_name      String?
    last_name       String?
    createdAt       DateTime @default(now()) @db.Timestamp(6)
}
